<!-- Feed Partial -->
<div class="container" style="padding:5px;overflow: hidden; max-width: 900px; height: 223px; margin-left: auto; margin-right: auto;">
	<h3><center>Live Feed</center></h3>
		<div class="jCarouselLite" style="">
			<ul style="margin: 0px; padding: 0px; position: relative; list-style-type: none; z-index: 1;">
				<% @feeds.each do |feed| %>
					<li style="overflow: hidden; float: left; width: 181px; height 200px;">	
						<div class="container" style="vertical-align: middle; background-color:white;width:150px;height:140px;border:2px solid black;float:left;padding:5px;margin:5px">
						<%= feed.username %> <br>
						<%= feed.message %> 
						</div>
					</li>
				<% end %>
			</ul>
		</div>	
</div>

